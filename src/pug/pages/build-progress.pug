    var class_page = 'class-page-build-progress'
    var breadcrumbsSubitem = [
      {
        label: 'Build-progress',
        link: pagesLink[0].build_progress
      }
    ]
    var cards = [
      {
        img: 'build-progress/2.png',
        date: '25.01.2021',
      }, {
        img: 'build-progress/2.png',
        date: '25.01.2021',
      }, {
